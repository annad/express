package express.range
{
	/**
	 * A wrapper that makes creating ranges more readable. For
	 * example, you can create a range of numbers with the following:
	 * <code>var range:IRange = from(0).to(10);</code>.
	 * 
	 * @param obj The <code>IRange.from</code> value.
	 * @return The object.
	 */
	public function from(obj:Object):Object
	{
		return obj;
	}
}